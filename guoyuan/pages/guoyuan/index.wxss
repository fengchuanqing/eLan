/* pages/guoyuan/guoyuan/index.wxss */
.container {
    padding-top: 8vh;
}
.item {
    width: 90%;
    height: 130px;
    margin: -10px auto;
    position: relative;
    padding-left: 10%;
    padding-top: 6%;
}

.text1 {
    font-size: 28px;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 1px;
    color: #ffffff;
    display: block;
}

.text2 {
    line-height: 32px;
    font-size: 16px;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 0px;
    color: #ffffff;
}

image {
    position: absolute;
    z-index: -1;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}