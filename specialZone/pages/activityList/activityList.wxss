.page {
  background-color: #f5f6f7;
  min-height: 100vh;
  padding: 32rpx;
  box-sizing: border-box;
}
.none{
  width: 305rpx;
  height: 416rpx;
  display: block;
  margin: 100rpx auto;
}
.activity_item {
  background-color: #ffffff;
  box-shadow: 0px 3rpx 10rpx 0px rgba(0, 0, 0, 0.07);
  border-radius: 10rpx;
  margin-top: 28rpx;
  padding: 18rpx 34rpx 24rpx 28rpx;
  color: #000000;
}
.activity_item_farm{
  font-size: 30rpx;
  color: #000000;
  display: flex;
  margin-bottom: 28rpx;
}
.activity_item_rate{
  font-size: 18rpx;
  color: #686b73;
  margin-top: 5rpx;
  margin-left: 20rpx;
  display: flex;
  align-items: center;
}
.activity_item_rate image{
  width: 25rpx;
  height: 23rpx;
  margin-left: 5rpx;
  vertical-align: middle;
}

.activity_item>image {
  width: 100%;
  height: 228rpx;
  background-color: #ffc700;
}

.activity_item_tit {
  font-size: 30rpx;
  margin-top: 24rpx;
}

.activity_item_con {
  margin-top: 14rpx;
  font-size: 24rpx;
  line-height: 31rpx;
}

.activity_item_btm {
  margin-top: 25rpx;
  display: flex;
  justify-content: space-between;
  font-size: 24rpx;
  color: #999999;
  align-items: center;
}

.activity_item_submit {
  width: 160rpx;
  height: 61rpx;
  background-color: #428ffc;
  border-radius: 30rpx;
  text-align: center;
  line-height: 61rpx;
  color: #ffffff;
}