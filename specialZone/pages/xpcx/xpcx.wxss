.search .van-search {
    background: unset !important;
}

.search .van-search__content {
    background: #fff;
    box-shadow: 0px 0px 24px 0px rgba(47, 111, 247, 0.1);
}

.navs {
    height: 162rpx;
    padding: 0 30rpx;
    background-color: #ffffff;
    box-shadow: 0px 0px 24rpx 0px rgba(47, 111, 247, 0.1);
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 20rpx;
}

.navsLeft {
    flex: 4;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.navs .nav {
    color: #363a44;
    font-size: 32rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}
.selectNav{
    font-weight: bold;
}

.navs .nav image {
    max-width: 58rpx;
    max-height: 52rpx;
    display: block;
    margin-bottom: 20rpx;
}

.goodsList {
    background-color: #ffffff;
    box-shadow: 0rpx 0rpx 24rpx 0rpx rgba(47, 111, 247, 0.1);
    padding: 0 30rpx;
}

.tit {
    border-bottom: 1rpx solid #e8e9ec;
    height: 68rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 32rpx;
    position: sticky;
    top: 0;
    background-color: #ffffff;
}

.tit>text {
    color: #363a44;
    font-weight: bold;
}

.tit .nav {
    display: flex;
    height: 100%;
    align-items: center;
    line-height: 68rpx;
}

.tit .nav>view {
    color: #b3b5b9;
    margin-left: 30rpx;
    position: relative;
    height: 100%;
}

.curNav {
    color: #428ffc !important;
    font-weight: bold;
}

.curNav::after {
    content: '';
    width: 100%;
    height: 2rpx;
    background-color: #428ffc;
    position: absolute;
    left: 0;
    bottom: 0;
}

.goods {
    padding: 33rpx 0 31rpx;
    border-bottom: 1rpx solid #e8e9ec;
}

.goodsTop {
    display: flex;
}

.goodsTop>image {
    width: 156rpx;
    height: 156rpx;
    background-color: #428ffc;
}

.goodsInfo {
    flex: 1;
    margin-left: 19rpx;
    font-size: 24rpx;
}

.goodsName {
    font-size: 28rpx;
    color: #363a44;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.goodsName text {
    font-weight: bold;
}

.goodsName image {
    width: 29rpx;
    height: 31rpx;
    margin-left: 21rpx;
}

.goDetail {
    color: #428ffc;
}

.tags {
    color: #b3b5b9;
    margin: 10rpx 0;
}

.gg {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.jg {
    font-size: 28rpx;
    color: #141414;
}

.jg text {
    color: #ff6a2a;
    font-weight: bold;
    font-style: italic;
}

.goOrder {
    width: 144rpx;
    height: 55rpx;
    background-color: #428ffc;
    border-radius: 28rpx;
    text-align: center;
    line-height: 55rpx;
    color: #ffffff;
}

.goOrder image {
    width: 21rpx;
    height: 21rpx;
    margin-left: 11rpx;
}

.goodsBtm {
    margin-top: 24rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 61rpx;
    background-color: rgba(66, 143, 252, .1);
    border-radius: 31rpx;
    padding: 0 30rpx;
    font-size: 32rpx;
    color: #363a44;
}

.goodsBtm text {
    color: #686b73;
}
.allPopup{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background-color: rgba(0,0,0,.4);
}
.contain{
    background-color: #ffffff;
    width: calc(100% - 64rpx);
    border-radius: 20rpx;
    position: absolute;
    top: 43%;
    left: 32rpx;
    transform: translateY(-50%);
    display: flex;
    align-items: flex-start;
    min-height: 539rpx;
}
.contain::before{
    content: '';
    width: 0;
    height: 0;
    border-bottom: 30rpx solid #ffffff;
    border-left: 30rpx solid transparent;
    border-right: 30rpx solid transparent;
    position: absolute;
    top: -27rpx;
    right: 15rpx;
}
.contain .nav{
    flex: 0.25;
    color: #363a44;
    font-size: 32rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding-top: 30rpx;
}
.contain .nav image {
    max-width: 58rpx;
    max-height: 52rpx;
    display: block;
    margin-bottom: 20rpx;
}