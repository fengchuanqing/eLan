.container {
    margin-top: 30rpx;
    background-color: #fff;
    padding: 30rpx 0;
}

.title {
    padding-left: 30rpx;
    font-size: 36rpx;
    font-weight: 500;
    line-height: 88rpx;
    font-stretch: normal;
    letter-spacing: 0rpx;
    color: #363a44;
}

.right_color {
    color: #428ffc!important;
}
.nscz_box {
    display: flex;
     flex-wrap: wrap;
}
.nscz_box view{
    margin: 20rpx;
    width: 148rpx;
	height: 60rpx;
    background-color: #b3b5b9;
    border-radius: 8rpx;
    font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 60rpx;
	letter-spacing: 0rpx;
    color: #ffffff;
    text-align: center;
}
.active {
    background-color: #428ffc!important;
}
.queren {
    position: fixed;
    bottom: 3%;
    left: 4%;
    width: 689rpx;
	height: 108rpx;
	background-color: #428ffc;
    border-radius: 54rpx;
    font-size: 36rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 108rpx;
	letter-spacing: 0rpx;
    color: #ffffff;
    text-align: center;
}
.picker_box {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100vw;
    height: 43vh;
    z-index: 999;
}
.pickerSty {
    width: 100%;
    height: 100%;
}