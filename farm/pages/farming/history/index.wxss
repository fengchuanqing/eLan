.nsjl_bpx {}

.nsjl {
    margin-top: 30rpx;
    padding: 0 2%;
    background-color: #ffffff;
    box-shadow: 0rpx 0rpx 24rpx 0rpx rgba(47, 111, 247, 0.1);
    background-color: #fff;
}

.title {
    font-size: 36rpx;
    font-weight: normal;
    font-stretch: normal;
    line-height: 114rpx;
    letter-spacing: 0rpx;
    color: #428ffc;
}

.title2 {
    width: 100%;
    margin: 0 auto;
    height: 114rpx;
    line-height: 114rpx;
    font-size: 36rpx;
    padding-left: 50rpx;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 0rpx;
    color: #363a44;
    background-color: rgb(236, 244, 255);
}

.item {
    display: flex;
    justify-content: space-around;
    line-height: 84rpx;
}

.item>text:nth-child(1) {
    text-align: center;
    width: 30%;
    font-size: 32rpx;
    font-weight: normal;
    letter-spacing: 0rpx;
    color: #428ffc;
}

.item>text:nth-child(2) {
    text-align: center;
    width: 60%;
    font-size: 32rpx;
    font-weight: normal;
    letter-spacing: 0rpx;
    color: #b3b5b9;
}

.tjns {
    position: fixed;
    left: 5%;
    bottom: 3%;
    width: 90%;
    height: 108rpx;
    line-height: 108rpx;
    text-align: center;
    font-size: 36rpx;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 0rpx;
    color: #ffffff;
    background-color: #428ffc;
    border-radius: 54rpx;
}
.kkry {
    margin: 30rpx auto;
    width: 248rpx;
    height: 356rpx;
}
.kkry image{
    width: 100%;
    height: 100%;
}