.yjtk,
.qctk {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0%;
  background-color: rgba(3, 3, 3, .5);
}

.section_yj {
  background-color: #fff;
  width: 90%;
  margin: 20% auto;
  padding: 3%;
  position: relative;
}

.section_yj_x {
  padding: 15rpx;
  font-size: 28rpx;
  position: absolute;
  right: 3%;
  top: 1%;
  cursor: pointer;
  transform: scale(1.5, 1);
}

.yj_title {
  font-size: 32rpx;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0rpx;
  color: #363a44;
}

