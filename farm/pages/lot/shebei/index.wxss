}

.yj_title3 {
  border-top: solid 1rpx #e8e9ec;
  margin: 20rpx 0;
  padding-top: 20rpx;
  font-size: 32rpx;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0rpx;
  color: #363a44;
}

.yj_title4 {
  font-size: 28rpx;
  font-weight: normal;
  font-stretch: normal;
