.zdcy_box{
  margin-top: 20rpx;
  background-color: #ffffff;
  padding: 30rpx;
}
.zdcy_tit{
  font-size: 30rpx;
  display: flex;
  justify-content: space-between;
}
.zdcy_child{
  display: flex;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #ebedf0;
}
.zdcy_child_item{
  flex: 1;
}
.zdcy_child_item_lab{
  font-size: 30rpx;
  display: flex;
  justify-content: space-between;
}
.jt{
  color: #333333;
  font-weight: bold;
  font-size: 20rpx;
}
.del{
  width: 40rpx;
  height: 40rpx;
  color: red;
  font-size: 40rpx;
  text-align: center;
  line-height: 38rpx;
  border: 1rpx solid red;
  border-radius: 5rpx;
}
.addChild{
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #428ffc;
  font-size: 30rpx;
}
.add{
  border: 1rpx solid #428ffc;
  color: #428ffc;
  margin-right: 10rpx;
}
.addCy{
  border: 1rpx solid #428ffc;
  background-color: #ffffff;
  margin: 50rpx 30rpx 0;
  border-radius: 50rpx;
}
.submit{
  margin: 30rpx 30rpx 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 50rpx;
  font-size: 32rpx;
  color: #ffffff;
  background-color: #428ffc;
  text-align: center;
  letter-spacing: 10rpx;
}