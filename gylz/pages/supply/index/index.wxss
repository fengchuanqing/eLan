
.menu{
  box-shadow: none !important;
}
.con{
  padding: 0 32rpx;
  background-color: #ffffff;
  margin-bottom: 150rpx;
}
.nyyd{
  display: flex;
  border-bottom: solid 1rpx #e8e9ec;
  padding: 30rpx 0;
}
.nyyd_l{
  position: relative;
  width: 180rpx;
	height: 180rpx;
}
.nyyd_l_sta{
  width: 97rpx;
	height: 34rpx;
	background-color: rgba(66,143,252,.75);
	border-top-right-radius: 17rpx;
  border-bottom-right-radius: 17rpx;
  color: #ffffff;
  position: absolute;
  left: 0;
  top: 11rpx;
  font-size: 24rpx;
  text-align: center;
}
.yiLz{
  background-color: rgba(255, 106, 42, .75);
}
.nyyd image{
  width: 100%;
	height: 100%;
	background-color: #00b578;
}
.nyyd_r{
  margin-left: 24rpx;
  width: calc(100% - 204rpx);
}
.nyyd_name{
  font-size: 32rpx;
  font-weight: bold;
}
.nyyd_tag_item{
  margin-right: 18rpx;
}
.nyyd_py{
  display: flex;
  width: 100%;
  justify-content: space-between;
  color: #686b73;
  margin: 11rpx 0 0;
}
.nyyd_py text:first-child{
  color: #e63633;
  font-size: 32rpx;
  font-style: italic;
}
.nyyd_tag{
  margin-top: 10rpx;
}
.ms{
  font-style: normal !important;
  color: #686b73 !important;
  font-size: 28rpx !important;
}
.btns{
  display: flex;
  justify-content: flex-end;
  margin-top: 20rpx;
}
.btns>view{
  margin-left: 19rpx;
  line-height: 56rpx;
  text-align: center;
  border-radius: 8rpx;
  flex-shrink: 0;
}
.qdlz{
  width: 161rpx;
	height: 56rpx;
	background-color: #428ffc;
  color: #ffffff;
}
.cxbj{
  width: 161rpx;
	height: 56rpx;
	background-color: #ffffff;
	border: solid 1rpx #428ffc;
  color: #428ffc;
}
.del{
	width: 107rpx;
	height: 56rpx;
	background-color: #ffffff;
  border: solid 1rpx #b3b5b9;
  color: #686b73;
}
.submit{
  width: 90%;
  height: 108rpx;
	background-color: #428ffc;
  border-radius: 54rpx;
  line-height: 108rpx;
  text-align: center;
  font-size: 36rpx;
  color: #ffffff;
  position: fixed;
  left: 5%;
  bottom: 33rpx;
}
.none{
  width: 305rpx;
  height: 416rpx;
  margin: 58rpx auto;
  display: block;
}