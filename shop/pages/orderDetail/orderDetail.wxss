/* pages/Merchant/orderDetail/orderDetail.wxss */
/* 地址 */
.container {
  padding-bottom: 150rpx;
  position: relative;
}

.box {
  width: 92%;
  margin: 0 auto;
}

.address {
  padding: 0 30rpx;
  position: relative;
  background-color: #fff;
}

.a_first {
  display: flex;
  align-items: center;
  font-size: 24rpx;
  font-weight: normal;
  font-stretch: normal;
  line-height: 60rpx;
  letter-spacing: 0rpx;
  color: #141414;
}

.a_second {
  font-size: 36rpx;
  font-weight: normal;
  font-stretch: normal;
  line-height: 60rpx;
  letter-spacing: 0rpx;
  color: #141414;
  font-weight: 700;
}

.a_third {
  font-size: 30rpx;
  font-weight: normal;
  font-stretch: normal;
  line-height: 60rpx;
  letter-spacing: 0rpx;
  color: #141414;
}

.a_line {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 4rpx;
}

.right_back {
  position: absolute;
  right: 3%;
  top: 30%;
}

.right_back>image {
  padding: 20rpx;
  width: 14rpx;
  height: 24rpx;
}

/* 地址以上 */
.d_type {
  padding: 35rpx 30rpx;
  font-weight: 700;
  font-size: 32rpx;
  font-stretch: normal;
  line-height: 42rpx;
  letter-spacing: 0rpx;
  color: #141414;
  background-color: #fff;
  border-bottom: 0.5px solid rgb(233, 231, 231);
}

.d_type>text {
  margin: 0 10rpx;
  font-size: 26rpx;
  font-weight: normal;
  font-stretch: normal;
  line-height: 46rpx;
  letter-spacing: 0rpx;
  color: #141414;
}

.item {
  padding: 20rpx 30rpx;
  background-color: #fff;
  position: relative;
  margin: 30rpx 0;
}

.goods{
  display: flex;
  position: relative;
  border-bottom: 1rpx solid #eeeeee;
  padding: 20rpx 0;
}
.i_text{
  flex: 1;
}
.i_text .gg{
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 32rpx;
  color: #363a44;
  margin-top: 5rpx;
}
.i_img>image {
  width: 120rpx;
  height: 120rpx;
  margin-right: 20rpx;
}

.i_text>view:nth-child(1) {
  font-size: 32rpx;
  font-weight: normal;
  font-stretch: normal;
  line-height: 50rpx;
  letter-spacing: 0rpx;
  color: #272727;
}

.i_text>view:nth-child(2) {
  font-size: 24rpx;
  font-weight: normal;
  font-stretch: normal;
  /* line-height: 88rpx; */
  letter-spacing: 0rpx;
  color: #666666;
}

.d_detail {
  background-color: #fff;
  padding-bottom: 20rpx;
}

.d_detail>view {
  font-size: 28rpx;
  font-weight: normal;
  font-stretch: normal;
  line-height: 42rpx;
  letter-spacing: 0rpx;
  display: flex;
}

.d_detail>view>text {
  color: #141414;
  width: 25%;
  margin: 10rpx 30rpx;
}

.d_detail>view>text:nth-child(1) {
  color: #999999;
  width: 18%;
}

.d_detail>view>text:nth-child(2) {
  width: 68%;
}

.d_bottom {
  width: 100%;
  position: absolute;
  z-index: 1;
  bottom: 2%;
  left: 0;
}

.d_b_qrsh {
  width: 678rpx;
  margin: 0 auto;
  height: 96rpx;
  background-color: #3377ff;
  border-radius: 48rpx;
  font-size: 30rpx;
  text-align: center;
  font-weight: normal;
  font-stretch: normal;
  line-height: 96rpx;
  letter-spacing: 0rpx;
  color: #ffffff;
}



.d_b_qrsh_b {
  display: flex;
justify-content: space-around;
}

.d_b_qrsh_s {
  height: 96rpx;
  width: 45%;
  border-radius: 48rpx;
  font-size: 30rpx;
  text-align: center;
  font-weight: normal;
  font-stretch: normal;
  line-height: 96rpx;
  letter-spacing: 0rpx;
}

.d_b_qrsh_b>input:nth-child(1) {
  color: rgb(51, 119, 255);
  background-color: #fff;
  border: 1px solid rgb(51, 119, 255);
}

.d_b_qrsh_b>input:nth-child(2) {
  color: #ffffff;
  background-color: #3377ff;
}

/* 填写物流 */
.show_wl {
  width: 600rpx;
  height: 550rpx;
  margin: 40% auto;
  background-color: #ffff;
  position: relative;
}

.s_w_title {
  width: 100%;
  text-align: center;
  font-size: 30rpx;
  font-weight: normal;
  font-stretch: normal;
  line-height: 109rpx;
  letter-spacing: 0rpx;
  color: #222222;
}

.sw_margin {
  width: 86%;
  margin: 0 auto;
  background-color: #f6f6f6;
  display: flex;
  align-content: center;
  padding-right: 10rpx;
}

.sw_margin>input {
  text-align: center;
  width: 100%;
  height: 85rpx;
  font-size: 32rpx;
  margin-right: 10rpx;
}

.s_w_bottom {
  width: 100%;
  height: 88rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  /* border-top: 0.5px solid #e9e3e3; */
  font-size: 28rpx;
  display: flex;
  text-align: center;
}

.s_w_bottom>input {
  width: 50%;
  height: 100%;
  color: #999999;
}

.s_w_bottom>input:nth-child(2) {
  color: #3377ff;
}

.d_b_delete {
  width: 266rpx;
  height: 96rpx;
  border: 1rpx solid #666666;
  border-radius: 48rpx;
  line-height: 96rpx;
  text-align: center;
  font-size: 30rpx;
  font-weight: normal;
  font-stretch: normal;
  line-height: 46rpx;
  letter-spacing: 0rpx;
  color: #222222;
  float: right;
  margin-right: 20rpx;
}

.d_b_qufahuo {
  width: 266rpx;
  height: 96rpx;
  border: 1rpx solid #666666;
  border-radius: 48rpx;
  line-height: 96rpx;
  text-align: center;
  font-size: 30rpx;
  font-weight: normal;
  font-stretch: normal;
  line-height: 46rpx;
  letter-spacing: 0rpx;
  background-color: #3377ff;
  color: #fff;
  float: right;
  margin-right: 20rpx;
}

.pz_tp {
  width: 300rpx;
  height: 200rpx;
}

input::-webkit-input-placeholder {
  color: #999999;
}

.overImg {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  max-width: 80%;
  height: 900rpx;
}