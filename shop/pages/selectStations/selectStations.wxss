}
.selectSearch {
  /* height: 134rpx; */
  /* display: flex; */
  padding: 0 30rpx;
  margin-top: 20rpx;
}
.selectSearch .van-search {
  width: 100%;
  background: #f5f6f7 !important;
  padding-left: 0;
  padding-right: 0;
}

.selectSearch .van-search__content {
  background: #fff;
  box-shadow: 0px 0px 10px 0px rgba(47, 111, 247, 0.1);
}
.list {
  padding: 0 32rpx;
  margin-bottom: 170rpx;
  margin-top: 20rpx;
}
.item {
  background-color: #fff;
  margin-bottom: 20rpx;
  border-radius: 20px;
  padding: 32rpx;
  display: flex;
}
.selectBox{
  width: 36rpx;
	height: 36rpx;
  border: solid 2rpx #b3b5b9;
  border-radius: 20rpx;
}
.curSelectBox{
  background-color: #428ffc;
  position: relative;
}
.curSelectBox::after{
  content: 'âˆš';
  color: #ffffff;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.itemTop{
  flex: 1;
  margin-left: 17rpx;
}
.imgBox {
  position: relative;
}
.imgBox > image {
  width: 100%;
  /* height: 311rpx; */
  background-color: #1989fa;
}
.imgBox > view {
  width: 118rpx;
  height: 58rpx;
  border-top-right-radius: 29rpx;
  border-bottom-right-radius: 29rpx;
  font-size: 36rpx;
  color: #ffffff;
  text-align: center;
  line-height: 58rpx;
  position: absolute;
  left: 0;
  top: 0;
}
.zwName {
  color: #428ffc;
}
.zwStatus {
  background-color: #428ffc;
}
.zwJy {
  background-color: #b3b5b9;
}
.xm {
  line-height: 60rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.xm>text{
  line-height: 40rpx;
}
.add{
  width: 100%;
  height: 128rpx;
  padding: 16rpx 36rpx 0;
	background-color: #ffffff;
  position: fixed;
  left: 0;
  bottom: 0;
}
.addBtn {
  width: 100%;
  height: 88rpx;
	background-color: #428ffc;
	border-radius: 44rpx;
  line-height: 88rpx;
  text-align: center;
  color: #ffffff;
}
.none{
  text-align: center;
  color: #b3b5b9;
  margin-top: 200rpx;
}
.none navigator{
  color: #428ffc;
  display: inline-block;
}