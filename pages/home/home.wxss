page {
  background-color: #f6f7f8;
  min-height: 100%;
}

.swiper {
  width: 100%;
  height: 335rpx;
  background: #fff;
}

.swiper-item {
  width: 100%;
  padding: 0 4%;
  height: 100%;
  position: relative;
  background: #fff;

}

.swiper-item .more {
  width: 220rpx;
  height: 57rpx;
  background-color: rgba(255, 255, 255, .57);
  text-align: center;
  line-height: 57rpx;
  color: #686b73;
  position: absolute;
  top: 271rpx;
  left: 100rpx;
}

.swiper-item image {
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
  background-color: #ffffff;
}

.notices {
  width: 100%;
  height: 99rpx;
  background-color: #ffffff;
  box-shadow: 0rpx 2rpx 24rpx 0rpx rgba(47, 111, 247, 0.16);
  display: flex;
  padding: 0 32rpx;
  align-items: center;
  margin-bottom: 23rpx;
}

.notices-left {
  position: relative;
  padding-right: 25rpx;
  color: #363a44;
}

.notices-left::after {
  content: '';
  width: 1rpx;
  height: 68rpx;
  background-color: #e8e9ec;
  position: absolute;
  top: 50%;
  right: 0;
  transform: translateY(-50%);
}

.notices-left text {
  color: #428ffc;
  display: block;
}

.notices-right {
  height: 100%;
  width: calc(100% - 50px);
}

.notices-swiper {
  width: 100%;
  height: 100%;
}

.notices-swiper-item {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #b3b5b9;
}

.notices-swiper-item text:first-child {
  margin-left: 26rpx;
  color: #686b73;
  width: 70%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.notices-swiper-item text:nth-child(2){
  width: 20%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.temp {
  width: 100%;
  height: 470rpx;
  background-color: #ffffff;
  padding: 0 32rpx;
  font-size: 32rpx;
  color: #363a44;
}

.temp-tit {
  padding: 28rpx 0;
  display: flex;
  flex-direction: column;
}

.temp-tit image {
  width: 94rpx;
  height: 6rpx;
}

.temp-con {
  width: 100%;
  display: flex;
  justify-content: space-between;
}

.temp-con1 {
  width: 100%;
}

.temp-con-left,
.temp-con-right {
  width: 44.67%;
}

.temp-con-left1,
.temp-con-right1 {
  width: 100%;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
}

.temp-con-item1 {
  width: 100%;
  height: 190rpx;

}

.temp-con-item1:first-child {
  margin-bottom: 19rpx;
}

.temp-con-item1 image {
  width: 100%;
  height: 100%;
}

.temp-con-item1-name {
  padding: 30rpx 0 0 35rpx;
  position: absolute;
  z-index: 1;
}

.temp-con-item1-name text {
  color: #5989f9;
  display: block;
}

.temp-con-item2-name {
  padding: 35rpx 0 0 36rpx;
}

.temp-con-item2-name text {
  color: #00b578;
}

.temp-con-item3 {
  width: 100%;
  height: 101rpx;
  border-radius: 10rpx;
  margin-bottom: 15rpx;

}

.temp-con-item3 .img {
  width: 100%;
  text-align: center;
}


.temp-con-item3 image {
  z-index: 0;
  width: 72rpx;
  height: 72rpx;
}

.temp-con-item3-name {
  text-align: center;
  z-index: 1;
}

.temp-con-item3-name text {
  color: #fba754;
}

.color1 {
  color: #f9605f !important;
}

.color2 {
  color: #ff6600 !important;
}

.openShop {
  padding: 0 32rpx;
  margin: 16rpx 0 29rpx;
  height: 155rpx;
}

.openShop image {
  width: 100%;
  height: 100%;
}

.temp-tabs {
  padding: 28rpx 0;
  display: flex;
}

.temp-tabs>view {
  display: flex;
  flex-direction: column;
  margin-right: 45rpx;
  color: #b3b5b9;
}

.temp-tab-act {
  color: #363a44 !important;
}

.temp-tabs image {
  width: 94rpx;
  height: 6rpx;
}

.temp-con-item4 {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(0, 181, 120, .15);
}

.temp-con-item4:first-child {
  margin-bottom: 19rpx;
  background-color: rgba(66, 143, 252, .15) !important;
}

.temp-con-item4 image {
  width: 156rpx;
  height: 156rpx;
}

.temp-con-item4 text {
  display: block;
}

.temp-con-item5 {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(209, 156, 79, .15);
  margin-bottom: 15rpx;
}

.temp-con-item5:first-child {
  background-color: rgba(92, 189, 215, .15) !important;
}

.temp-con-item5:nth-child(3) {
  margin-bottom: 0;
  background-color: rgba(255, 106, 42, .15) !important;
}

.temp-con-item5 image {
  width: 103rpx;
  height: 102rpx;
}

.temp2-con {
  width: 100%;
}

.service-item {
  padding: 31rpx 0;
  width: 100%;
  border-bottom: 1rpx solid #e8e9ec;
}

.service-item:nth-child(2) {
  border-bottom: none;
}

.service-item .navigator {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.service-item image {
  width: 128rpx;
  height: 99rpx;
  background-color: #9ccbd3;
}

.service-text {
  width: 339rpx;
  height: 74rpx;
  font-family: PingFang-SC-Regular;
  font-weight: normal;
  font-stretch: normal;
  line-height: 48rpx;
  letter-spacing: 0rpx;
  color: #363a44;
}

.service-text view:nth-child(2) {
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.service-type {
  width: 133rpx;
  height: 45rpx;
  border-radius: 23rpx;
  text-align: center;
  line-height: 45rpx;
  font-size: 24rpx;
}

.fzbz {
